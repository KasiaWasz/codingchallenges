import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        int [] input = {3,5,6,8,1};
        int j = -1;
        int temp;
        for(int i=0; i<input.length; i++){
            if(input[i]%2 != 0){
                j++;
                temp = input[i];
                input[i] = input[j];
                input[j] = temp;
            }
        }for(int number: input){
        System.out.print(number);
    }
}}
